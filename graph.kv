#:kivy 2.0
              
<GraphScreen>:
    Label:
        text: 'GRAPH'
        pos: 200,200
    Button:
        text: 'Back to menu'
        size_hint: None, None
        size: 100, 50
        on_press:
            root.manager.transition.direction = 'right'
            root.manager.current = 'menu'