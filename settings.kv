#:kivy 2.0

<SettingScreen>:

    Label:
        padding_x: dp(10)
        id: time
        text: '00:00:00'
        font_name: 'fonts/dd.otf'
        pos: 200,200


    Button:
        text: 'Back to menu'
        size_hint: None, None
        size: 100, 50
        on_press:
            root.manager.transition.direction = 'right'
            root.manager.current = 'menu'